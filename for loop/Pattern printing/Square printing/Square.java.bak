class  Square
{
	public static void main(String[] args) 
	{   
		for(int i=1; i<=5; i++)
		{
			for(int j=1; j<=6-i; j++)
			{
				System.out.print(" ");
		    }
              System.out.print("*");
			  for (int k=2; k<2*i-1; k++ )
			  {

				  System.out.print(" ");
			  }
			  if(i>=2)
			{
              System.out.print("*");
			}
			  System.out.println("");
			  
		}
		System.out.print("* * * * *");
	}
}
