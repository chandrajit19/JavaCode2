// finding the value of base and power of positive integers
import java.util.*;
class Fact 
{   
	public static void main(String[] args) 
	{ Scanner s=new Scanner(System.in);
	System.out.println("Enter base  integer = ");
	int a=s.nextInt();
	System.out.println("Enter power integer = ");
	int b=s.nextInt();
	 int fact=1;
	for(int i=1; i<=b; i++)
		{
         fact*=a;      // fact=fact*a,   both are same
		}
         System.out.println(fact);
	}
	
}
