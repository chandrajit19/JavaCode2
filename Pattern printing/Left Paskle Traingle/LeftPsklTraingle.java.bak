// left paskal's traingle
class  LeftPsklTraingle
{
	public static void main(String[] args) 
	{
		for(int i=1; i<=9; i++)
		if(i>=1 && i<=5)
		{
            for(int j=1; j<=5-i; j++)
			{
				System.out.print("  ");
			}
			for(int k=1; k<=i; k++)
			{
				System.out.print("* ");
			}
			System.out.println();
		}
		else
		{
			for(int j=1; j<=i-5; j++)
			{
				System.out.print("  ");
			}
			for(int k=1; k<=10-i; k++)
			{
				System.out.print("* ");
			}
			System.out.println();
	}
}
